  "bugs": {
    "url": "https://github.com/JayPuff/browser-file-storage/issues"
  },
  "homepage": "https://github.com/JayPuff/browser-file-storage#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  }
}
